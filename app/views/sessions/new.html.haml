- provide(:title,'Log in!')
%h1 Log in!
.row
	.span6.offset3
		= form_for(:session, url: login_path) do |f|
			= f.label :email
			= f.text_field :email

			= f.label :password
			= f.password_field :password

			= f.submit "Log in!", class: "btn btn-large btn-primary"
		%br
		%p 
			Nuevo usuario?,
			= link_to "Registrece ahora!", registro_path
